{
    "extends": [
        "eslint:recommended"
    ],
    "env": {
        "browser": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
        }
    },
    "rules": {
        "semi": "error",
        "eqeqeq": "error",
        "prefer-const": "warn",
        "no-console": "warn"
    }
}
